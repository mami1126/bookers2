<main>
<div class="container px-5 px-sm-0">
 <div class="row">
  
  <div class="col-md-3">
  <%= render 'list', user: @user %>
  <%= render 'books/newbook', book: @book %>
  </div>

  <div class="col-md-8 offset-md-1 pt-3">
   <h2>Users</h2>
   <table class="table">
    <thead>
     <th>image</th>
     <th>name</th>
     <th colspan="3"></th>
     </thead>

    <tbody>
     <% @users.each do |user| %>
      <tr>
       <td><%= image_tag user.get_profile_image(100,100) %></td>
       <td><%= user.name %></td>
       <td><%= link_to "Show", user_path(user) %></td>
      </tr>
     <% end %>
    </tbody>
   </table>
  </div>
 </div>
</div>
</main>